{
  "homepage": "https://haminhhanh.github.io/cms-xlaunch",
  "private": true,
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.3.1",
    "@mui/material": "^5.3.1",
    "@mui/styled-engine-sc": "^5.3.0",
    "@umijs/hooks": "^1.9.3",
    "@umijs/preset-react": "1.x",
    "dayjs": "^1.10.7",
    "ethers": "^5.5.2",
    "gh-pages": "^3.2.3",
    "lodash": "^4.17.21",
    "numeral": "^2.0.6",
    "rc-checkbox": "^2.3.2",
    "rc-dropdown": "^3.2.0",
    "rc-field-form": "^1.22.0",
    "rc-menu": "^9.3.1",
    "rc-pagination": "^3.1.15",
    "rc-switch": "^3.2.2",
    "rc-table": "^7.21.0",
    "recoil": "^0.5.2",
    "styled-components": "^5.3.3",
    "umi": "^3.4.15",
    "umi-request": "^1.4.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.178",
    "@types/numeral": "^2.0.2",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/test": "^3.4.15",
    "imagemin": "^8.0.1",
    "imagemin-webp": "^7.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "react": "17.x",
    "react-dom": "17.x",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
